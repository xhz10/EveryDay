    <input placeholder="许洪卓" size="50">
<!--    size拓宽单行文本框-->
    <input value="许洪卓" maxlength="8" readonly><!--只读-->
    <input type="password"><!--实际上是以明文传输到服务器的-->
    <textarea rows="20"></textarea>
